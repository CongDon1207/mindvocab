# Server & Database
PORT=5001
MONGODB_CONNECTION_STRING=mongodb+srv://username:password@cluster.example.mongodb.net/vocab

# Import pipeline
IMPORT_MAX_SIZE_MB=5
# Batch enrich nhỏ giúp giảm timeout/cắt phản hồi từ AI khi nhập nhiều từ
IMPORT_ENRICH_BATCH=10
IMPORT_STORAGE_DIR=./storage/imports
IMPORT_TEMP_DIR=./storage/tmp

# AI provider configuration
AI_PROVIDER=gemini
AI_FALLBACKS=
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-flash
# Tăng timeout & retry để cải thiện tỷ lệ thành công cho batch lớn
AI_TIMEOUT_MS=45000
AI_RETRY_LIMIT=3
